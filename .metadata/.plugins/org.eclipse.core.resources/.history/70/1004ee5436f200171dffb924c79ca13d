package program2;
import java.util.Scanner;


public class PrimeNumber {

	public static void main(String[] args) {
		int count=0;
		Scanner in = new Scanner(System.in);
		int num = in.nextInt();
		in.close();
		for(int i=2;i<num;i++)
		{
			if((num%i==0))
				count++;			
		}
		if (count>1)
			System.out.println(num+" is not a prime number");
		else
		System.out.println(num+ " is a prime number");
		
	}

}
